 pod 'IQKeyboardManager'
 pod 'SVProgressHUD'


  # Pods for Kanakia

  target 'KanakiaTests' do
    inherit! :search_paths
    # Pods for testing
 pod 'Alamofire'
 pod 'Firebase/Core'
 pod 'Firebase/Messaging'
 pod 'Firebase/Database'
 pod 'GoogleMaps'
 pod 'DropDown'
 pod 'IQKeyboardManager'
 pod 'SVProgressHUD'
 
  end

  target 'KanakiaUITests' do
    inherit! :search_paths
    # Pods for testing
 pod 'Alamofire'
 pod 'Firebase/Core'
 pod 'Firebase/Messaging'
 pod 'Firebase/Database'
 pod 'GoogleMaps'
 pod 'DropDown'
 pod 'IQKeyboardManager'
 pod 'SVProgressHUD'
  end

end
